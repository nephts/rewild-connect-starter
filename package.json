{
  "name": "rewild-connect",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:watch": "vite build --watch",
    "typecheck:watch": "tsc -b --watch --preserveWatchOutput",
    "pages:dev": "wrangler pages dev dist --local",
    "dev:worker": "npm-run-all --parallel typecheck:watch build:watch pages:dev",
    "test": "vitest run",
    "preview": "vite preview"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cookie": "^1.0.2",
    "framer-motion": "^11.0.0",
    "hono": "^4.9.7",
    "lucide-react": "^0.441.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.26.2",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250918.0",
    "@types/node": "^22.5.4",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "vite": "^5.4.6",
    "vitest": "^1.2.1",
    "wrangler": "^4.40.0"
  }
}
